{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 1000000000
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "min": 0
    },
    "imageUrl": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "merchant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::merchant.merchant",
      "inversedBy": "products"
    }
  }
}
